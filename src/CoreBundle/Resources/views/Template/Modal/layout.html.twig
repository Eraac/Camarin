<!-- Modal Structure -->
<div id="{{ modal_id }}" class="modal">
    <div class="modal-content">
        <h4>{{ modal_title|trans }}</h4>

        {% block modal_content %}
        {% endblock %}
    </div>

    <div class="modal-footer">
        <a href="#!" class=" modal-action modal-close waves-effect btn-flat">{{ "core.close"|trans }}</a>
    </div>
</div>
